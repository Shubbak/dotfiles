[alias]
	ca = commit
	s = status
	df = diff
	dc = diff --cached
    amend = commit --amend --no-edit
    lastlog = log -1 HEAD
    uncommit = reset --soft HEAD~1
    grap = log --all --decorate --oneline --graph
[init]
	defaultBranch = main
