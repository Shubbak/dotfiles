[url "git@github.com:"]
	insteadOf = https://github.com/
[user]
	email = a.shubbak@outlook.com
	name = Abdulrahman Shubbak
[alias]
	ca = commit -am
    cm = commit -m
	s = status
	df = diff
	dc = diff --cached
    amend = commit --amend --no-edit
    lastlog = log -1 HEAD
    uncommit = reset --soft HEAD~1
    graph = log --all --decorate --oneline --graph
	checkAlias = config --get-regexp alias
[init]
	defaultBranch = main
[core]
	autocrlf = input
	editor = nvim
[pull]
	rebase = false
[help]
    autocorrect = 30
